file = {:C$\backslash$:/Dropbox/resistance LSTM/resistance{\_}papers/Petzoldetal2013.pdf:pdf},
isbn = {0022-0493},
issn = {0022-0493},
journal = {Journal of economic entomology},
keywords = {Animals,Bacillus thuringiensis,Bacillus thuringiensis: genetics,Beetles,Beetles: drug effects,Beetles: genetics,Beetles: growth {\&} development,Endotoxins,Endotoxins: genetics,Endotoxins: metabolism,Genetically Modified,Genetically Modified: genetics,Genetically Modified: growth {\&} development,Illinois,Insecticide Resistance,Insecticides,Insecticides: pharmacology,Iowa,Larva,Larva: drug effects,Larva: genetics,Larva: growth {\&} development,Nebraska,Plant Roots,Plant Roots: genetics,Plant Roots: growth {\&} development,Plants, Genetically Modified,Plants, Genetically Modified: genetics,Plants, Genetically Modified: growth {\&} development,Seasons,Species Specificity,Zea mays,Zea mays: genetics,Zea mays: growth {\&} development},
